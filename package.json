{
  "name": "moa-backend3",
  "type": "module",
  "version": "2.1.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "node --loader ts-node/esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "node --loader ts-node/esm -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r newrelic dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.0.5",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.5",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.5",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "@sentry/node": "^7.69.0",
    "apple-signin-auth": "1.7.5",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "date-fns": "^2.30.0",
    "env-cmd": "^10.1.0",
    "fb": "2.0.0",
    "firebase-admin": "^11.9.0",
    "google-auth-library": "8.9.0",
    "hbs": "^4.2.0",
    "helmet": "^7.0.0",
    "ms": "^2.1.3",
    "multer": "1.4.4",
    "nanoid": "^5.0.1",
    "nestjs-i18n": "10.2.6",
    "nestjs-s3": "^2.1.0-next.1",
    "newrelic": "^11.1.0",
    "nodemailer": "6.9.3",
    "nodemailer-mailgun-transport": "^2.1.5",
    "p-retry": "^6.0.0",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "4.0.1",
    "pg": "8.11.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "sqlite3": "^5.1.6",
    "square": "^30.0.0",
    "stripe": "^12.12.0",
    "swagger-ui-express": "4.6.3",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^4.13.0",
    "twitter": "1.7.1",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.7",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "4.17.17",
    "@types/facebook-js-sdk": "3.3.6",
    "@types/inline-css": "^3.0.1",
    "@types/jest": "29.5.2",
    "@types/luxon": "^3.3.2",
    "@types/ms": "^0.7.31",
    "@types/multer": "1.4.7",
    "@types/node": "18.16.19",
    "@types/nodemailer": "^6.4.10",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/twitter": "1.7.1",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.6.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
