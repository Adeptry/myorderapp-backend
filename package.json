    "@types/bcryptjs": "^2.4.6",
    "@types/express": "4.17.21",
    "@types/inline-css": "^3.0.3",
    "@types/jest": "29.5.11",
    "@types/ms": "^0.7.34",
    "@types/multer": "1.4.11",
    "@types/node": "20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemailer-mailgun-transport": "^1.4.6",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "3.0.13",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "sqlite3": "^5.1.6",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "license": "UNLICENSED",
  "name": "moa-backend3",
  "private": true,
  "scripts": {
    "build": "nest build",
    "clean": "rimraf dist",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:show": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:show",
    "schema:sync": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:sync",
    "prebuild": "rimraf dist",
    "prepare": "is-ci || husky install",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "node --loader ts-node/esm -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node -r newrelic dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "node --loader ts-node/esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "type": "module",
  "version": "2.5.12"
}
